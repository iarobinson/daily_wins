<main role="main">
  <% if current_user %>
    <%= render "layouts/nav_for_user_settings" %>
    <%= render "users/avatar_list_of_followings" %>
    <%= render "users/wins_from_followings" %>
    <hr>
  <% else %>
    <%= render "new_user_view" %>
    <%= render "list_challenges", challenges: @challenges %>
  <% end %>
</main>
