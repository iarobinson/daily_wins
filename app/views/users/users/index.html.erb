<div class="container">
  <div class="row pb-5">

    <div class="<%= standard_column_setting %> py-3">
      <% @users.each do |user| %>
        <div class="col-12">
          <%= link_to user_path(user) do %>
          <%= render user %>
          <% end %>
        </div>
      <% end %>
    </div>

  </div>
</div>
