<div class="container">
  <div class="row py-3">
    <div class="col-md-6 offset-md-3 pb-5">
      <ul class="list-group">
        <% @users.each do |user| %>
          <%= link_to user_path(user), class: "list-group-item list-group-item-action" do %>
            <%= render user %>
          <% end %>
        <% end %>
      </ul>
    </div>
  </div>
</div>
